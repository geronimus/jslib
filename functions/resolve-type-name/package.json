  },
  "homepage": "https://github.com/geronimus/jslib#readme",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.30.0",
    "eslint-watch": "^7.0.0",
    "eslint-webpack-plugin": "^2.5.4",
    "mocha": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
